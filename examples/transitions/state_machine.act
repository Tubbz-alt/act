behaviour init of StateMachine
interface constructor()

creates
  uint256 x := 0

invariants
    x <= 2

behavior f of StateMachine
interface f()

case x == 0:

  storage
    x => 1

behavior g of StateMachine
interface g()

case x == 1:

  storage
    x => 2

behavior h of StateMachine
interface h()

case x == 2:

  storage
    x => 0
